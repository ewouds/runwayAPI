        {
          "name": "Basic Airport Search",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Accept",
                "value": "application/json"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/api/v1/airports/search?q=London&limit=5&format=simple&fuzzy=true",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "airports", "search"],
              "query": [
                {
                  "key": "q",
                  "value": "London",
                  "description": "Search term (required)"
                },
                {
                  "key": "limit",
                  "value": "5",
                  "description": "Maximum results (1-100)"
                },
                {
                  "key": "format",
                  "value": "simple",
                  "description": "Response format: simple or full"
                },
                {
                  "key": "fuzzy",
                  "value": "true",
                  "description": "Enable fuzzy matching"
                }
              ]
            },
            "description": "Search across all airport fields with optional fuzzy matching"
          }
        },
        {
          "name": "Fuzzy Search with Scoring",
